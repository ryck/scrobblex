{
  "name": "scrobblex",
  "description": "Self-hosted app that enables Plex / Tautulli scrobbling integration with Trakt via webhooks",
  "version": "1.5.1",
  "homepage": "https://github.com/ryck/scrobblex",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryck/scrobblex.git"
  },
  "bugs": {
    "url": "https://github.com/ryck/scrobblex/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "src/index.js",
  "type": "module",
  "license": "MIT",
  "author": "Ricardo Gonzalez <ryck@ryck.me>",
  "keywords": [
    "plex",
    "trakt",
    "webhook",
    "scrobbler",
    "express"
  ],
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "css": "npx @tailwindcss/cli -i ./views/pages/input.css -o ./static/output.css --watch",
    "lint": "eslint src/**/*.js",
    "format": "eslint src/**/*.js --fix",
    "pretty": "prettier --write \"src/**/*.js\""
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.0",
    "eslint": "^9.28.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.8",
    "axios": "^1.9.0",
    "axios-cache-interceptor": "^1.8.0",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "lodash": "^4.17.21",
    "multer": "^2.0.1",
    "node-localstorage": "^3.0.5",
    "tailwindcss": "^4.1.8",
    "winston": "^3.17.0"
  }
}
