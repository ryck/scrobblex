{
  "name": "scrobblex",
  "description": "Self-hosted app that enables Plex scrobbling integration with Trakt via webhooks",
  "version": "0.1.0",
  "main": "src/index.js",
  "type": "module",
  "license": "MIT",
  "author": "Ricardo Gonzalez <ryck@ryck.me>",
  "keywords": [
    "plex",
    "trakt",
    "webhook",
    "scrobbler",
    "express"
  ],
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint src/**/*.js",
    "format": "eslint src/**/*.js --fix",
    "pretty": "prettier --write \"src/**/*.js\""
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "nodemon": "^3.1.3",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "dependencies": {
    "axios": "^1.7.2",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.17.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-localstorage": "^3.0.5",
    "winston": "^3.13.0"
  }
}
