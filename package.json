{
  "name": "scrobblex",
  "description": "Self-hosted app that enables Plex scrobbling integration with Trakt via webhooks",
  "version": "1.4.1",
  "homepage": "https://github.com/ryck/scrobblex",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryck/scrobblex.git"
  },
  "bugs": {
    "url": "https://github.com/ryck/scrobblex/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "src/index.js",
  "type": "module",
  "license": "MIT",
  "author": "Ricardo Gonzalez <ryck@ryck.me>",
  "keywords": [
    "plex",
    "trakt",
    "webhook",
    "scrobbler",
    "express"
  ],
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "css": "npx @tailwindcss/cli -i ./views/pages/input.css -o ./static/output.css --watch",
    "lint": "eslint src/**/*.js",
    "format": "eslint src/**/*.js --fix",
    "pretty": "prettier --write \"src/**/*.js\""
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.5",
    "eslint": "^9.21.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.0.8",
    "axios": "^1.7.9",
    "axios-cache-interceptor": "^1.6.2",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-localstorage": "^3.0.5",
    "tailwindcss": "^4.0.12",
    "winston": "^3.17.0"
  }
}
